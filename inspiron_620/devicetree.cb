			device ref pci_bridge off
			end
			device ref lpc on
				subsystemid 0x1028 0x04ed
			end
			device ref sata1 on
				subsystemid 0x1028 0x04ed
			end
			device ref smbus on
				subsystemid 0x1028 0x04ed
			end
			device ref sata2 on # Unsupported PCI device 8086:1c08
				subsystemid 0x1028 0x04ed
			end
			device ref thermal off
			end
		end
	end
