			register "pcie_port_coalesce" = "1"
			register "sata_interface_speed_support" = "0x3"
			register "sata_port_map" = "0x1"
			register "spi_lvscc" = "0x2005"
			register "spi_uvscc" = "0x2005"
			register "usb_port_config" = "{
				{1, 7, 0},
				{1, 7, 0},
				{1, 7, 1},
				{1, 7, 1},
				{1, 7, 2},
				{1, 7, 2},
				{1, 7, 3},
				{1, 7, 3},
				{1, 7, 4},
				{1, 7, 4},
				{1, 7, 5},
				{1, 7, 5},
				{1, 7, 6},
				{1, 7, 6},
			}"
			device ref mei1 on
				subsystemid 0x1028 0x04ed
			end
			device ref mei2 off
			end
			device ref me_ide_r off
			end
			device ref me_kt off
			end
			device ref gbe off
			end
			device ref ehci2 on
				subsystemid 0x1028 0x04ed
			end
			device ref hda on
				subsystemid 0x1028 0x04ed
			end
			device ref pcie_rp1 on
				subsystemid 0x1028 0x04ed
			end
			device ref pcie_rp2 on
				subsystemid 0x8086 0x1c12
			end
			device ref pcie_rp3 off
			end
			device ref pcie_rp4 off
			end
			device ref pcie_rp5 on
				subsystemid 0x1028 0x04ed
